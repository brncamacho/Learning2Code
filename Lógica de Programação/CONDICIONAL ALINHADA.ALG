algoritmo "CONDICIONAL ALINHADA"
var
   dinheiro: real
inicio
   Escreva ("Quanto dinheiro tenho? ")
   Leia(dinheiro)
   Se (dinheiro >= 10000) então
      Escreva ("Vou à Disney")
   senão
      se (dinheiro >= 5000) e (dinheiro < 10000) então
         Escreva ("Visitar Feira Popular")
      senão
         Escreva ("Fico em casa")
      Fimse
   Fimse

fimalgoritmo